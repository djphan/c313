compile			= "stopandwait.c"

bandwidth		= 56Kbps,

messagerate             = 1000ms,
propagationdelay        = 2500ms,

probframeloss		= 0,
probframecorrupt	= 0,

host perth {
    x=100 y=50
    winx=0, winy=50
    link to R1 {
    }
}

router R1 {east east of perth, 
            address= 12, 
            link to R2}

router R2 {east east east of perth, 
            address= 13, 
            link to sydney}

host sydney {
    east east east east east of perth
    winx=550, winy=50
}
